{
  "ci": {
    "collect": {
      "numberOfRuns": 1,
      "url": [
        "http://localhost:4173/CV/"
      ],
      "startServerCommand": "npm run preview -- --host 0.0.0.0 --port 4173"
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "warn",
          {
            "minScore": 0.8
          }
        ],
        "categories:accessibility": [
          "error",
          {
            "minScore": 0.95
          }
        ],
        "categories:best-practices": [
          "error",
          {
            "minScore": 0.9
          }
        ],
        "categories:seo": [
          "warn",
          {
            "minScore": 0.9
          }
        ]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
